{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "widget/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/widget/(.*)", "dest": "/widget/$1" },
    { "src": "/", "dest": "/widget/index.html" }
  ]
}
